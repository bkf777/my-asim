{
  "name": "react16-webpack-app",
  "version": "0.1.0",
  "private": true,
  "description": "React 16 app bundled with Webpack 5",
  "main": "dist/index.html",
  "scripts": {
    "start": "webpack serve --mode development --open --hot",
    "build": "webpack --mode production",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "format": "prettier --write \"{src,public}/**/*.{ts,tsx,js,jsx,css,html,json,md}\"",
    "format:check": "prettier --check \"{src,public}/**/*.{ts,tsx,js,jsx,css,html,json,md}\""
  },
  "dependencies": {
    "amis": "^6.13.0",
    "amis-editor": "^6.13.0",
    "copy-to-clipboard": "^3.3.3",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-hook-form": "^7.39.3",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.15",
    "@types/react": "^16.14.68",
    "@types/react-dom": "^16.9.25",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@types/react-router": "5.0.1",
    "@types/react-router-dom": "^5.3.3",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-webpack-plugin": "^5.5.3",
    "prettier": "^3.7.4",
    "sass": "^1.97.2",
    "sass-loader": "^16.0.6",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.4",
    "typescript": "^5.5.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}